	<queryString>
		<![CDATA[SELECT equi.nombre,'Alquilado' as Estado,equi.cantidad,ka.fecha,obra.nombre as nombreObra,obra.almacenista,obra.zona,clie.empresa,clie.nombre,clie.telefono,clie.direccion FROM
obra, detalle_equipo deta,kardex ka, equipo equi, cliente clie
where
ka.id_obra=obra.id_obra and
ka.id_equipo=equi.id_equipo and
deta.id_equipo=equi.id_equipo and
obra.id_cliente=clie.id_cliente and
deta.estado_alquiler ='A' and
ka.fecha between $P{fechaInicio} and $P{fechaFin}
and ka.id_obra=$P{obra}]]>
	</queryString>
